{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/index.js",
      "runtimeExecutable": "/usr/local/bin/node",
      "env": {
        "NO_RETRY": "true"
      }
    }
  ]
}
